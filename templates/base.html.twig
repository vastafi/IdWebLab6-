<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>

        {% block stylesheets %}
            <link href="{{ asset('assets/main/css/style.css') }}" type="text/css" rel="stylesheet" />

       <!-- Bootstrap4 files-->
        <link href="{{ asset('assets/main/css/bootstrap.min.css') }}"type="text/css" rel="stylesheet" />


        <!-- custom style -->
        <link href="{{ asset('assets/main/css/ui.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('assets/main/css/responsive.css') }}" rel="stylesheet" media="only screen and (max-width: 1200px)" />

        <!-- our styles -->

        <link href="{{ asset('assets/main/css/styles.css') }}" rel="stylesheet" type="text/css" />


        {% endblock %}

        {% block javascripts %}
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
